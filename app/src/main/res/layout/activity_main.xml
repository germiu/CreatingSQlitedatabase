<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tx_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="id"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="24dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/et_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="58dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_id"/>

    <TextView
        android:id="@+id/tx_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Username"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="107dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/et_id"/>

    <EditText
        android:id="@+id/et_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="140dp"
        app:layout_constraintStart_toStartOf="@+id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_username" />

    <TextView
        android:id="@+id/tx_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="189dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/et_username"/>

    <EditText
        android:id="@+id/et_Phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="210dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_phone"/>

    <TextView
        android:id="@+id/tx_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="263dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/et_Phone"
        />

    <EditText
        android:id="@+id/et_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="287dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_email"
         />

    <TextView
        android:id="@+id/tx_door"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Door"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="334dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/et_email" />

    <EditText
        android:id="@+id/et_door"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="353dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_door"/>

    <TextView
        android:id="@+id/tx_place"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Place"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="402dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/et_door"
         />

    <EditText
        android:id="@+id/et_place"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="427dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_place" />

    <TextView
        android:id="@+id/tx_country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Country"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="488dp"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/et_place"/>

    <EditText
        android:id="@+id/et_country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="@id/tx_id"
        app:layout_constraintTop_toBottomOf="@id/tx_country"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="444dp" />

    <Button
        android:id="@+id/bt_savedat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SAVE DATA"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="489dp"
        app:layout_constraintTop_toBottomOf="@id/et_country"/>

    <Button
        android:id="@+id/bt_display"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Display"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="537dp"
        app:layout_constraintTop_toBottomOf="@id/bt_savedat"/>
</androidx.constraintlayout.widget.ConstraintLayout>